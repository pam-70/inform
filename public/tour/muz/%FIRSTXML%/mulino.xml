      if(device.flash,
        set(spotsound37_started, true);
        set(spotsound37_paused, false);
        playsound3D(spotsound37, "zhuravli.mp3", -127.616040, -1.326599, 90, get(volume), 0);
      );
      if(device.flash,
        set(spotsound38_started, true);
        set(spotsound38_paused, false);
        playsound3D(spotsound38, "vstavaj_strana_ogromnaya.mp3", 26.293330, -17.244898, 90, get(volume), 0);
      );
    );
  </action>
  <action name="stoppanolocalsounds">
    if(device.flash,set(spotsound37_started, false);set(spotsound37_paused, false);stopsound(spotsound37););
    if(device.flash,set(spotsound38_started, false);set(spotsound38_paused, false);stopsound(spotsound38););
  </action>
  <action name="setpanolocalsoundsvolume">
    if (tour_soundson,
      div(volume, tour_soundsvolume, 100);
    );
  </action>
  <action name="pausepanolocalsounds">
    if(spotsound37_started,
      ifnot(spotsound37_paused,
        if(device.flash,set(spotsound37_paused, true);pausesound(spotsound37););
      );
    );
    if(spotsound38_started,
      ifnot(spotsound38_paused,
        if(device.flash,set(spotsound38_paused, true);pausesound(spotsound38););
      );
    );
  </action>
  <action name="resumepanolocalsounds">
    if(tour_soundson,
      div(volume, tour_soundsvolume, 100);

      set(killDirectionnalSound, false);
      for(set(hotspotSoundi, 0), hotspotSoundi LT hotspot.count, inc(hotspotSoundi),
        if(hotspot[get(hotspotSoundi)].pauseothersounds,
          if(hotspot[get(hotspotSoundi)].stateVideo == 'playing',
            set(killDirectionnalSound, true);
          );
        );
      );
      ifnot (killDirectionnalSound,
      if(spotsound37_started,
        if(spotsound37_paused,
        if(device.flash,set(spotsound37_paused, false);resumesound(spotsound37););
        );
      ,

        if(device.flash,
          set(spotsound37_started, true);
          set(spotsound37_paused, false);
          playsound3D(spotsound37, "zhuravli.mp3", -127.616040, -1.326599, 90, get(volume), 0);
        );
      );
      if(spotsound38_started,
        if(spotsound38_paused,
        if(device.flash,set(spotsound38_paused, false);resumesound(spotsound38););
        );
      ,

        if(device.flash,
          set(spotsound38_started, true);
          set(spotsound38_paused, false);
          playsound3D(spotsound38, "vstavaj_strana_ogromnaya.mp3", 26.293330, -17.244898, 90, get(volume), 0);
        );
      );
      );
    ,
    );
  </action>
