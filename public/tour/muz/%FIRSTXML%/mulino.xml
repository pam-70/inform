      ifnot(spotsound39_paused,
        if(device.flash,set(spotsound39_paused, true);pausesound(spotsound39););
      );
    );
    if(spotsound40_started,
      ifnot(spotsound40_paused,
        if(device.flash,set(spotsound40_paused, true);pausesound(spotsound40););
      );
    );
  </action>
  <action name="resumepanolocalsounds">
    if(tour_soundson,
      div(volume, tour_soundsvolume, 100);

      set(killDirectionnalSound, false);
      for(set(hotspotSoundi, 0), hotspotSoundi LT hotspot.count, inc(hotspotSoundi),
        if(hotspot[get(hotspotSoundi)].pauseothersounds,
          if(hotspot[get(hotspotSoundi)].stateVideo == 'playing',
            set(killDirectionnalSound, true);
          );
        );
      );
      ifnot (killDirectionnalSound,
      if(spotsound39_started,
        if(spotsound39_paused,
        if(device.flash,set(spotsound39_paused, false);resumesound(spotsound39););
        );
      ,

        if(device.flash,
          set(spotsound39_started, true);
          set(spotsound39_paused, false);
          playsound3D(spotsound39, "zhuravli.mp3", -42.247442, -12.564180, 90, get(volume), 0);
        );
      );
      if(spotsound40_started,
        if(spotsound40_paused,
        if(device.flash,set(spotsound40_paused, false);resumesound(spotsound40););
        );
      ,

        if(device.flash,
